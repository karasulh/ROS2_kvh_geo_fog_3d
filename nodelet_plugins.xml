<!--
    Library path is where your package's library is located.
    This is likely "lib/lib<nodelet library name>", where
    <nodelet library name> is the name of the library generated
    in your CMakeLists.txt
-->
<library path="lib/libkvh_geo_fog_3d_driver_nodelet">
  <!--
      name: @(package)/@(class name), e.g. "ros_example/Nodelet"
      type: 2(namespace)::@(class name), e.g. "example::Nodelet"
      base_class_type: "nodelet::Nodelet"
  -->
  <class name="kvh_geo_fog_3d_driver/Nodelet" type="kvh::Nodelet" base_class_type="nodelet::Nodelet">
  <description>
    Nodelet for the KVH GEO FOG 3D Driver.
  </description>
  </class>
</library>
